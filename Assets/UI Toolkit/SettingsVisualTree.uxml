                <ui:Slider high-value="1" name="MainVolume" style="width: 300px;" />
            </ui:VisualElement>
            <ui:VisualElement name="Music" style="flex-grow: 0; flex-direction: row; justify-content: flex-end; width: 500px; -unity-background-image-tint-color: rgba(255, 255, 255, 0);">
