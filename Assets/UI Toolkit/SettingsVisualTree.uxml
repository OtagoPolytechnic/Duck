            <ui:VisualElement name="SFX" style="flex-grow: 0; flex-direction: row; width: 500px; justify-content: flex-end; -unity-background-image-tint-color: rgba(255, 255, 255, 0);">
                <ui:Label tabindex="-1" text="Sound Effects" parse-escape-sequences="true" display-tooltip-when-elided="true" class="text" style="font-size: 22px;" />
